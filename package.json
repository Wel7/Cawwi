{
  "name": "cawwi",
  "version": "1.0.0",
  "description": "A discord bot to follow some vtuber music release",
  "main": "src/index.js",
  "type":"module",
  "scripts": {
    "build": "tsc && cp ./src/.env ./dist/.env",
    "build:watch": "tsc --watch",
    "start": "npm run build && node dist/index.js",
    "start:pm2": "npm run build && pm2 start ecosystem.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wel7/Cawwi.git"
  },
  "keywords": [
    "vtuber",
    "vtubers",
    "music"
  ],
  "author": "wellle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wel7/Cawwi/issues"
  },
  "homepage": "https://github.com/Wel7/Cawwi#readme",
  "dependencies": {
    "@types/node": "^20.11.20",
    "axios": "^1.6.7",
    "discord-api-types": "^0.37.71",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
